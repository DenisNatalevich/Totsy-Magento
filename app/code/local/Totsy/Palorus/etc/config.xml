<config>
    <modules>
        <Totsy_Palorus>
            <version>0.1.2</version>
        </Totsy_Palorus>
    </modules>
    <global>
        <blocks>
            <palorus>
                <rewrite>
                    <list>Totsy_Palorus_Block_Vault_List</list>
                </rewrite>
            </palorus>
        </blocks>
        <models>
            <palorus>
                <rewrite>
                    <vault>Totsy_Palorus_Model_Vault</vault>
                </rewrite>
            </palorus>
        </models>
        <helpers>
            <palorus>
                <rewrite>
                    <data>Totsy_Palorus_Helper_Data</data>
                </rewrite>
            </palorus>
        </helpers>
    </global>
    <frontend>
        <routers>
            <totsy_palorus>
                <use>standard</use>
                <args>
                    <module>Totsy_Palorus</module>
                    <frontName>palorus</frontName>
                </args>
            </totsy_palorus>
        </routers>
    </frontend>
</config>
